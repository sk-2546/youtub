{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.11"
    }
  },
  "rewrites": [
    { "source": "/search", "destination": "/api/index/search" },
    { "source": "/search/:path*", "destination": "/api/index/search" },
    { "source": "/playlist/:playlistId", "destination": "/api/index/playlist/:playlistId" },
    { "source": "/health", "destination": "/api/index/health" },
    { "source": "/api/:path*", "destination": "/api/:path*" }
  ]
}
